<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="318.0" prefWidth="495.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.neosvet.chat.client.connect.ConnectController">
    <children>
        <TextField fx:id="hostField" layoutX="161.0" layoutY="60.0" onAction="#connect">
            <font>
                <Font size="24.0"/>
            </font>
        </TextField>
        <TextField fx:id="portField" layoutX="161.0" layoutY="159.0" onAction="#connect">
            <font>
                <Font size="24.0"/>
            </font>
        </TextField>
        <Label layoutX="28.0" layoutY="68.0" text="Host">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <Label layoutX="28.0" layoutY="167.0" text="Port">
            <font>
                <Font size="24.0"/>
            </font>
        </Label>
        <Button layoutX="196.0" layoutY="258.0" mnemonicParsing="false" onAction="#connect" text="Connect">
            <font>
                <Font size="24.0"/>
            </font>
        </Button>
        <Label layoutX="28.0" layoutY="20.0" fx:id="statusLabel">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
